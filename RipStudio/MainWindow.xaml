    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup x:Name="LG1" DisplayName="{DynamicResource MenuLinkGroups_Welcome}" >
            <mui:LinkGroup.Links>
                <mui:Link x:Name="LG11" DisplayName="{DynamicResource Welcome_Link_0}" Source="/Pages/Introduction.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup x:Name="LG2" DisplayName="{DynamicResource MenuLinkGroups_AviSynth}">
            <mui:LinkGroup.Links>
                <mui:Link x:Name="LG21" DisplayName="{DynamicResource AviSynth_Link_0}" Source="Pages/ScriptCreater/Main.xaml" />
                <mui:Link x:Name="LG22" DisplayName="{DynamicResource AviSynth_Link_1}" Source="Pages/ScriptCreater/Video.xaml" />
                <mui:Link x:Name="LG23" DisplayName="{DynamicResource AviSynth_Link_2}" Source="Pages/ScriptCreater/Audio.xaml" />
