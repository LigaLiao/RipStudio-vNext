            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup x:Name="LG3" DisplayName="{DynamicResource MenuLinkGroups_Video}">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="H.264" Source="Pages/Video/H264Page.xaml" />
                <mui:Link DisplayName="H.265" Source="Pages/Video/H265Page.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup x:Name="LG4" DisplayName="{DynamicResource MenuLinkGroups_Audio}">
