        </mui:LinkGroup>
        <mui:LinkGroup x:Name="LG5" DisplayName="{DynamicResource MenuLinkGroups_Mux}">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="MP4" Source="Pages/Muxer/mp4.xaml" />
                <mui:Link DisplayName="MKV" Source="Pages/Muxer/mkv.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup x:Name="LG6" DisplayName="{DynamicResource MenuLinkGroups_Extended}">
            <mui:LinkGroup.Links>
                <mui:Link x:Name="LG61" DisplayName="{DynamicResource Extended_Link_0}" Source="Pages/Extended/MediaInfoPG.xaml" />
                <!--<mui:Link DisplayName="章节创建器" Source="Pages/Extended/ChapterEditor.xaml" />-->
                <mui:Link x:Name="LG62" DisplayName="{DynamicResource Extended_Link_1}"  Source="Pages/Extended/ApictureEncode.xaml" />
                <mui:Link x:Name="LG63" DisplayName="{DynamicResource Extended_Link_2}"  Source="Pages/Extended/EasyEncode.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup x:Name="LG7" DisplayName="{DynamicResource TitleLinks_1}" GroupKey="Settings">
            <mui:LinkGroup.Links>
                <mui:Link  Source="Pages/Settings/SettingsPage.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
        <mui:LinkGroup x:Name="LG8" DisplayName="{DynamicResource TitleLinks_2}" GroupKey="JobQueue">
            <mui:LinkGroup.Links>
                <mui:Link  Source="Pages/JobQueue.xaml" />
            </mui:LinkGroup.Links>
