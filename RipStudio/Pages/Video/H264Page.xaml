<UserControl x:Class="RipStudio.Pages.Video.H264Page"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}" Margin="0,28,0,0">
        <mui:ModernTab SelectedSource="Pages/Video/H264IO.xaml"  Layout="Tab" >
            <mui:ModernTab.Links>
                <mui:Link DisplayName="I/O" Source="Pages/Video/H264IO.xaml"  />
                <mui:Link DisplayName="Settings" Source="Pages/Video/H264SettingsTabPage.xaml"  />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
